<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>

<TabPane fx:id="tabPane" prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jrm.fx.ui.MainFrameController">
   <tabs>
      <Tab fx:id="profilePanelTab" closable="false" text="%MainFrame.Profiles">
         <content>
            <fx:include fx:id="profilePanel" source="ProfilePanel.fxml" />
         </content>
      </Tab>
      <Tab fx:id="scannerPanelTab" closable="false" text="%MainFrame.Scanner">
         <content>
            <fx:include fx:id="scannerPanel" source="ScannerPanel.fxml" />
         </content>
      </Tab>
   </tabs>
</TabPane>
