<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jrm.fx.ui.BatchDirUpd8rResultsController">
   <center>
      <ScrollPane fitToHeight="true" fitToWidth="true" prefHeight="400.0" prefWidth="600.0">
         <content>
            <TableView fx:id="resultList">
              <columns>
                <TableColumn fx:id="datCol" editable="false" maxWidth="800.0" minWidth="200.0" prefWidth="400.0" sortable="false" text="DAT/XML" />
                <TableColumn fx:id="haveCol" editable="false" maxWidth="80.0" minWidth="30.0" prefWidth="40.0" sortable="false" text="Have" />
                  <TableColumn fx:id="createCol" editable="false" maxWidth="80.0" minWidth="30.0" prefWidth="40.0" sortable="false" text="Create" />
                  <TableColumn fx:id="fixCol" editable="false" maxWidth="80.0" minWidth="30.0" prefWidth="40.0" sortable="false" text="Fix" />
                  <TableColumn fx:id="missCol" editable="false" maxWidth="80.0" minWidth="30.0" prefWidth="40.0" sortable="false" text="Miss" />
                  <TableColumn fx:id="totalCol" editable="false" maxWidth="80.0" minWidth="30.0" prefWidth="40.0" sortable="false" text="Total" />
                  <TableColumn fx:id="reportCol" editable="false" maxWidth="100.0" minWidth="70.0" prefWidth="70.0" sortable="false" text="Report" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </content>
      </ScrollPane>
   </center>
   <bottom>
      <HBox alignment="CENTER_RIGHT" prefHeight="20.0" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="ok" alignment="CENTER_RIGHT" mnemonicParsing="false" text="OK" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </bottom>
</BorderPane>
